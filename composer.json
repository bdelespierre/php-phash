        "bdelespierre",
        "phash",
        "image hashing algorithm"
    ],
    "homepage": "https://github.com/bdelespierre/php-phash",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Benjamin Delespierre",
            "email": "benjamin.delespierre@gmail.com",
            "role": "Developer"
        }
